{
    "latest": {
        "id": "latest",
        "tag": "^(latest)$",
        "symlink": "vanilla"
    },
    "vanilla": {
        "tag": "^(vanilla-[\\w\\d._-]+)$",
        "startup": {
            "done": ")! For help, type ",
            "userInteraction": [
                "Go to eula.txt for more info."
            ]
        },
        "stop": "stop",
        "configs": {
            "server.properties": {
                "server-ip=": "server-ip=0.0.0.0",
                "enable-query=": "enable-query=true",
                "server-port=": "server-port={{ build.default.port }}",
                "query.port=": "query.port={{ build.default.port }}"
            }
        },
        "output": {
            "find": "(\r\n)",
            "flags": "gm",
            "replace": "\n"
        },
        "log": {
            "custom": false,
            "location": "logs/latest.log"
        },
        "query": "minecraftping"
    },
    "spigot": {
        "tag": "^(spigot)$",
        "symlink": "vanilla"
    },
    "bungeecord": {
        "tag": "^(bungeecord)$",
        "symlink": "vanilla",
        "startup": {
            "done": "[INFO] Listening on "
        },
        "stop": "end",
        "configs": {
            "config.yml": {
                "  query_enabled:": "  query_enabled: true",
                "  query_port:": "  query_port: {{ build.default.port }}",
                "  host:": "  host: 0.0.0.0:{{ build.default.port }}"
            }
        },
        "log": {
            "location": "proxy.log.0"
        }
    }
}
