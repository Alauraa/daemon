{
  "extends": "airbnb",
  "rules": {
      "indent": [2, 4],
      "func-names": 0,
      "strict": 0,
      "quote-props": 0,
      "consistent-return": 0,
      "prefer-arrow-callback": 0,
      "max-len": 0,
      "prefer-template": 0,
      "no-control-regex": 0,
      "no-param-reassign": 0
  }
}
